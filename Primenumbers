package com.java.assignment1;

public class Primenumbers 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		
		int i=2;
		while(i>0&&i<=100)
		{
			if((i%2==0&&i>2)||(i%3==0&&i>3)||(i%5==0&&i>5)||(i%7==0&&i>7))
			i=i+0;
			else 
			System.out.print( " "+i );
			i++;
		}	
				
	}
}
